<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="FishAppPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>Logica.Album</class>
    <class>Logica.Enfermedad</class>
    <class>Logica.FichaDatos</class>
    <class>Logica.FichaMedica</class>
    <class>Logica.Habitante</class>
    <class>Logica.Imagen</class>
    <class>Logica.Medicion</class>
    <class>Logica.Parametro</class>
    <class>Logica.Pecera</class>
    <class>Logica.PeceraDulce</class>
    <class>Logica.PeceraMarina</class>
    <class>Logica.Usuario</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3307/fishapp?serverTimezone=America/Argentina/Buenos_Aires"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
